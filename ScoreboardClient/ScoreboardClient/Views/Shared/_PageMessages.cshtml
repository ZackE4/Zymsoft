@model List<ScoreboardClient.Models.PageMessage>
@if(Model.Count > 0)
{
    <div class="messageBox">
        <ul class="messageList">
            @foreach(var message in Model)
            {
                string listClass = "list" + message.Type.ToString();
                <li class="@listClass">@message.Message</li>
            }
        </ul>
    </div>
}